{
    "name": "flankerspb/curl",
    "type": "library",
    "description": "Simple cURL wrapper",
    "keywords": ["curl"],
    "homepage": "https://github.com/flankerspb/curl",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vitaliy Moskalyuk",
            "email": "flanker@bk.ru"
        }
